+import { useState, useRef, memo, useEffect } from "react";
+import { View, ScrollView, Alert, Dimensions } from "react-native";
+import { QuestionType, fetch_questions } from "./qaFetching";
+import { QA_Panel } from "./QA_Panel";
+import { TopBar } from "./TopBar";
+import { SideButtons } from "./SideButtons";
+import { correct, wrong, time, skip, q_total } from "../../../App";
+
+const sh = Dimensions.get('window').height;
+
+export const ForYou = memo(function(){
+   //console.log("rendering ForYou");
+   
+   let svr = useRef(null);
+   let iid = useRef(999999);
+   const [started, setStarted] = useState(false);
+   const [seconds, setSeconds] = useState(30);
+   const [qa, setQA]:[Array<QuestionType>,Function] = useState([]);
+   const [qIndex, setQIndex] = useState(0);
+   const [lastQIndex, setLastQIndex] = useState(0);
+   const [score, setScore] = useState(skip*q_total);      //  correct: +20       wrong: -20        skip: -10        full: 5*20=100
+   const [qAnswered, setQAnswered] = useState(0);
+
+   function test_ended(){
+      setStarted(false);
+      clearInterval(iid.current as any);
+      iid.current = null as any;      
+      Alert.alert(`(Correct:+${correct}%. Wrong:${wrong}%. \nSkipped:${skip}%)`);
+      Alert.alert(`Your score for the last 5 MCQs is: ${score}%.`);
+   }
+
+   useEffect(()=>{
+      fetch_questions(q_total, setQA, qa.map(qq=>qq.id));   //  load questions before user starts. removes repeated questions. may get less than 5 questions.
+   },[]);
+
+   useEffect(()=>{
+      if (started && qa.length > qIndex && (iid.current===null || iid.current===999999)) {
+         (svr.current as any).scrollTo({x:0, y:lastQIndex*sh, animate:true});
+         setQIndex(lastQIndex);
+         setLastQIndex(i=>(i+q_total));
+         setSeconds(time);
+         setScore(skip*q_total);
+         setQAnswered(0);
+         iid.current = setInterval(()=>{    // wait for questions to load first before starting countdown
+            setSeconds(s=>s-1);
+         },1000) as any;
+      } else if (!started){      // test stopped halfway
+         setQAnswered(99999);
+      }
+      if (started && qIndex+q_total > qa.length) fetch_questions(q_total, setQA, qa.map(qq=>qq.id));   //  each test consists of 5 questions. at least 5 questions loaded.
+   },[started,qa,qIndex]);
+
+   useEffect(()=>{
+      if (qAnswered>=q_total) {
+         test_ended();
+         return;
+      }
+      if (seconds<1){
+         setSeconds(0);   // in case negative
+         test_ended();
+      }
+   },[seconds]);
+
+   return (
+      <View style={{ flex: 1, backgroundColor: "black" }} >
+         <ScrollView ref={svr} style={{ height:qa.length*sh, opacity:(iid.current===999999)?0:1}} onScrollEndDrag={e=>{   // make view opaque as test starts
+               if ((e.nativeEvent.velocity as any).y  < 0){
+                  if (qIndex==qa.length) {
+                     Alert.alert("No more question available...");
+                     return;
+                  }
+                  if (qIndex+1==lastQIndex) {
+                     (svr.current as any).scrollTo({x:0, y:(lastQIndex-1)*sh, animate:true});   // bounce back to normal position towards the very last screen
+                     return;
+                  }
+                  (svr.current as any).scrollTo({x:0, y:(qIndex+1)*sh, animate:true});
+                  setQIndex(i=>(i+1));
+
+               } else {
+                  if (qIndex==0) return;
+                  (svr.current as any).scrollTo({x:0, y:(qIndex-1)*sh, animate:true});
+                  setQIndex(i=>(i-1));
+               }
+         }}>
+               {qa.map((q:QuestionType,index:number)=>(
+                  <QA_Panel key={index} question={q} setScore={setScore} countScore={ (qIndex>=lastQIndex-q_total) } setQAnswered={ setQAnswered }  />
+               ))}
+         </ScrollView>
+         <TopBar seconds={seconds} started={started} setSeconds={setSeconds} />
+         <SideButtons started={started} setStarted={setStarted} />          
+      </View>
+   );
+});
