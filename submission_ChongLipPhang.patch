+export type AnswerType = {
+    id:string, 
+    answer:string
+};
+
+export type QuestionType = {
+   id:number, 
+   question:string, 
+   answer:string,
+   description: string,
+   tag: string,
+   playlist: string,
+   unit: string,
+   user: {name: string, avatar:string},
+   image?: string,
+   options: AnswerType[]
+};
+
+function randomSwapA(arr:AnswerType[]){
+   let i:number = Math.floor(arr.length * Math.random());
+   let j:number = Math.floor(arr.length * Math.random());
+   let tmp:AnswerType = arr[i];
+   arr[i] = arr[j];
+   arr[j] = tmp;
+}
+
+function getQuestion():Promise<QuestionType>{
+   return new Promise((resolve,reject)=>{
+      fetch('https://cross-platform.rp.devfactory.com/for_you')
+      .then(response => response.json())
+      .then(json => {
+         for (let i=0; i<10; i++) randomSwapA(json.options);   // randomize order of answer options
+         let d = json.description.split("#");
+         json.description = d[0];
+         json.tag = d[1];
+         json.unit = "Unit " + json.description.split(".")[0];
+         resolve(json);
+      })
+      .catch(error => {
+         console.error("ERROR:"+error);
+         reject(error);
+      });
+   });
+}
+
+function getAnswer(id:number):Promise<{id:number, correct_options:[{id:string}]}>{
+   return new Promise((resolve,reject)=>{
+      fetch(`https://cross-platform.rp.devfactory.com/reveal?id=${id}`)
+      .then(response => response.json())
+      .then(json => {
+         //console.log(id,json);
+         resolve(json);
+      })
+      .catch(error => {
+         console.error("ERROR:"+error);
+         reject(error);
+      });
+   });
+}
+
+async function fetchQA():Promise<QuestionType>{
+   let question:QuestionType = await getQuestion();
+   let answer:{id:number, correct_options:[{id:string}]}= await getAnswer(question.id);
+   question.answer = answer.correct_options[0].id;
+   return question;
+};
+
+async function fetchQA_batch(n:number):Promise<[QuestionType]>{
+   let qs:[QuestionType] = [] as any;
+   let qsp:[QuestionType] = [] as any;
+   for (let i=0; i<n; i++) qsp.push(fetchQA() as any);
+   let r = await Promise.all(qsp);
+   for (let qa of r){
+      if (!qs.map(q=>q.id).includes(qa.id)) qs.push(qa);
+   }
+   return qs;
+}
+
+export function fetch_questions(n:number, setQA:Function, qids:number[]){  // may add less than n questions due to repeated questions fetched, but at least one question added.
+   fetchQA_batch(n).then(q=>{
+      q = q.filter(qq=>(!qids.includes(qq.id))) as [QuestionType];       // excludes repeated questions
+      if (q.length == 0 as number) fetch_questions(n, setQA, qids);      // refetch questions if no new question fetched
+      else {
+         setQA((qa:any)=>[...qa, ...q]);
+      }
+   });
+}
