+import { useState } from "react";
+import { View, Text, Dimensions, StyleSheet, TouchableHighlight } from "react-native";
+import { Image } from "expo-image";
+import { QuestionType } from "./qaFetching";
+import { OutlineText } from "../../shared/OutlineText";
+import { correct, wrong, skip } from "../../../App";
+
+const sh = Dimensions.get('window').height;
+
+export function OptionsList({question, setScore, countScore, setQAnswered, flex}:{question:QuestionType, setScore:Function, countScore:boolean, setQAnswered:Function, flex:number}){
+   const [playerAns, setPlayerAns] = useState("");
+   return (     // target background opacity divided by 8 due to 8 overlapping layers
+      <View style={{ flex: flex }}>
+         {question.options.map((o=>
+            <TouchableHighlight style={{ flex: 1, justifyContent: 'flex-start'}} key={o.id}  underlayColor="transparent" onPress={()=>{
+               if (countScore) {
+                  setScore((s:number)=>(s+((o.id==question.answer)?(correct-skip):(wrong-skip))));
+                  setQAnswered((n:number)=>(n+1));   // count score if only part of the current test, ie. last 5 questions.
+               }
+               if (playerAns==="") {
+                  setPlayerAns(o.id);
+               }
+            }}>
+               <View style={{flexDirection:"row"}}>
+                  <View style={{flex:99}}>
+                     <OutlineText stroke={ 2 } 
+                                 color="black" 
+                                 backgroundColor={((playerAns===question.answer && playerAns===o.id)?"rgba(40,177,143,0.0875)"
+                                                  :(playerAns!==question.answer && playerAns===o.id)?"rgba(220,95,95,0.0875)"
+                                                  :(playerAns!==""        && question.answer===o.id)?"rgba(40,177,143,0.0875)"
+                                                                                                    :"rgba(255,255,255,0.0625)")}>   
+                        <Text style={styles.option}>
+                           &nbsp;{o.answer}
+                        </Text>
+                     </OutlineText>
+                  </View>
+                  <View style={{ flex:1, 
+                                 position:"relative", 
+                                 top:(playerAns===o.id && playerAns===question.answer)?sh*0.03:sh*0.06, 
+                                 left:(playerAns===o.id && playerAns===question.answer)?"-70%":"20%", 
+                                 transform:(playerAns===o.id && playerAns===question.answer)?"rotateZ(0deg)":"rotateZ(180deg)"}}>
+                     { (playerAns===o.id)?((playerAns===question.answer)?
+                                          <Image source={require('../../../public/correct.gif')} contentFit="contain" style={styles.indicator}/>
+                                          :<Image source={require('../../../public/wrong.gif'  )} contentFit="contain" style={styles.indicator}/>
+                                          ):null
+                     }
+                  </View>
+               </View>
+            </TouchableHighlight>
+         ))}
+      </View>
+   );
+}
+
+const styles = StyleSheet.create({
+   option: {
+      marginLeft:"5%", 
+      padding:10,
+      paddingLeft: 0,
+      fontSize:sh*0.022, 
+      color:"white",
+   },
+   indicator:{
+      width:sh*0.1,
+      height:sh*0.1,
+   }
+});
